  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "array-flatten": "^3.0.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "encodeurl": "^2.0.0",
    "escape-html": "^1.0.3",
    "methods": "^1.1.2",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "parseurl": "^1.3.3",
    "proxy-addr": "^2.0.7",
    "range-parser": "^1.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
