{
  "name": "okane",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "postinstall": "npx prisma generate --schema=packages/db/prisma/schema.prisma",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "cd packages/db && npx prisma generate && cd ../..",
    "db:migrate": "prisma migrate dev --schema=packages/db/prisma/schema.prisma",
    "db:push": "prisma db push --schema=packages/db/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=packages/db/prisma/schema.prisma",
    "start-user-app": "cd ./apps/user-app && npm run start"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "prettier": "^3.2.5",
    "prisma": "^7.3.0",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@upstash/redis": "^1.36.1",
    "array-flatten": "^3.0.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "encodeurl": "^2.0.0",
    "escape-html": "^1.0.3",
    "methods": "^1.1.2",
    "next": "15.5.9",
    "next-auth": "^4.24.11",
    "parseurl": "^1.3.3",
    "proxy-addr": "^2.0.7",
    "range-parser": "^1.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.4",
    "serve-static": "^2.2.0",
    "utils-merge": "^1.0.1"
  },
  "version": "1.0.0",
  "description": "This Turborepo starter is maintained by the Turborepo core team.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tahircode/okane3.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Tahircode/okane/issues"
  },
  "homepage": "https://github.com/Tahircode/okane#readme"
}
