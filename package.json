{
  "name": "week-18",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "postinstall": "npx prisma generate --schema=packages/db/prisma/schema.prisma",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
